<config>
	<modules>
        <Delorum_Lightspeedcontent>
            <version>0.1.0</version>
        </Delorum_Lightspeedcontent>
    </modules>
    <!-- <global></global> -->
	<frontend>
	   <routers>
			<lightspeedcontent>
				<use>standard</use>
				<args>
					<module>Delorum_Lightspeedcontent</module>
					<frontName>lightspeedcontent</frontName>
				</args>
			</lightspeedcontent>
		</routers>
		<layout>
			<updates>
				<lightspeedcontent>
					<file>Delorum_Lightspeedcontent.xml</file>
				</lightspeedcontent>
			</updates>
		</layout>
		<!--
		<translate>
            <modules>
                <Delorum_Lightspeedcontent>
                    <files>
                        <default>Delorum_Lightspeedcontent.csv</default>
                    </files>
                </Delorum_Lightspeedcontent>
            </modules>
        </translate>
		-->
	</frontend>
</config>